<script src="~/Scripts/views/product.js"type="text/javascript"></script>
<div id="toast-render">
    @if (TempData["toast"] != null)
    {
        foreach (Toast toast in TempData["toast"] as List<Toast>)
        {
            <script>Toast('@toast.type','@toast.title','@toast.message')</script>
        }
    }
    @{
        TempData.Clear();
    }
</div>
<script src="~/Scripts/views/Styles.js" type="text/javascript"></script>
</body>
</html>